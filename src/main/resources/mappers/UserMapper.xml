<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.monco.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.monco.entity.User">
    <result column="id" property="id" />
    <result column="create_name" property="createName" />
    <result column="create_date" property="createDate" />
    <result column="update_name" property="updateName" />
    <result column="update_date" property="updateDate" />
    <result column="data_status" property="dataStatus" />
    <result column="deleted" property="deleted" />
        <result column="username" property="username" />
        <result column="nick_name" property="nickName" />
        <result column="password" property="password" />
        <result column="security_key" property="securityKey" />
        <result column="version" property="version" />
        <result column="create_id" property="createId" />
        <result column="update_id" property="updateId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_name,
        create_date,
        update_name,
        update_date,
        data_status,
        deleted,
        username, nick_name, password, security_key, version, create_id, update_id
    </sql>

</mapper>
